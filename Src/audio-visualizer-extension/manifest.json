{
  "name": "Playtify",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "An extension for audio visualization in spotify.",
  "icons": {
    "128": "icons/wv128x128.png"
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [ "lib/jquery.min.js", "content-scripts/content.js" ]
    }
  ],
  "web_accessible_resources": [
    "icons/pt128x128.png"
  ]
}