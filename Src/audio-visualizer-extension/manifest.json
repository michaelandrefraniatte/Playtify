{
  "manifest_version": 3,
  "name": "Playtify",
  "version": "0.0.0.1",
  "description": "An extension for audio visualization in spotify.",
  "permissions": [
      "scripting",
      "tabs"
  ],
  "host_permissions": [
      "http://*/*",
      "https://*/*"
  ],
  "background": {
      "service_worker": "background.js"
  },
  "icons": {
    "128": "icons/wv128x128.png"
  }
}